<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.backup_college_scheduler.ui.CourseUI.CourseFragment"
        android:label="@string/title_courses"
        tools:layout="@layout/fragment_courses" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.backup_college_scheduler.ui.AssignmentUI.AssignmentFragment"
        android:label="@string/title_assignments"
        tools:layout="@layout/fragment_assignments" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.backup_college_scheduler.ui.ExamUI.ExamFragment"
        android:label="@string/title_exams"
        tools:layout="@layout/fragment_exams" />

    <fragment
        android:id="@+id/navigation_todo"
        android:name="com.example.backup_college_scheduler.ui.Todo.TodoFragment"
        android:label="@string/title_exams"
        tools:layout="@layout/fragment_exams" />
</navigation>